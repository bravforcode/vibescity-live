{
  "__inputs": [
    {
      "name": "DS_LOKI",
      "label": "Loki",
      "description": "Grafana Loki datasource",
      "type": "datasource",
      "pluginId": "loki",
      "pluginName": "Loki"
    },
    {
      "name": "DS_BIGQUERY",
      "label": "BigQuery",
      "description": "Grafana BigQuery datasource",
      "type": "datasource",
      "pluginId": "grafana-bigquery-datasource",
      "pluginName": "BigQuery"
    }
  ],
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": {
          "type": "grafana",
          "uid": "-- Grafana --"
        },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "description": "Synthetic monitor + route SLO + trend signals exported from GitHub Actions into BigQuery and Loki.",
  "editable": true,
  "fiscalYearStartMonth": 0,
  "graphTooltip": 1,
  "id": null,
  "links": [],
  "liveNow": false,
  "panels": [
    {
      "datasource": {
        "type": "loki",
        "uid": "${DS_LOKI}"
      },
      "description": "Raw CI observability events pushed to Loki. Labels: service, workflow, lane, classification, status.",
      "fieldConfig": {
        "defaults": {},
        "overrides": []
      },
      "gridPos": {
        "h": 10,
        "w": 24,
        "x": 0,
        "y": 0
      },
      "id": 1,
      "options": {
        "dedupStrategy": "none",
        "enableLogDetails": true,
        "prettifyLogMessage": true,
        "showCommonLabels": false,
        "showLabels": false,
        "showTime": true,
        "sortOrder": "Descending",
        "wrapLogMessage": true
      },
      "targets": [
        {
          "datasource": {
            "type": "loki",
            "uid": "${DS_LOKI}"
          },
          "editorMode": "code",
          "expr": "{service=\"vibecity-ci\", workflow=~\"$workflow\", lane=~\"$lane\", classification=~\"$classification\", status=~\"$status\"} | json",
          "queryType": "range",
          "refId": "A"
        }
      ],
      "title": "Loki: CI Observability Logs",
      "type": "logs"
    },
    {
      "datasource": {
        "type": "grafana-bigquery-datasource",
        "uid": "${DS_BIGQUERY}"
      },
      "description": "Synthetic overall status derived from `release_health_events` (hourly buckets).",
      "fieldConfig": {
        "defaults": {
          "unit": "percent"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 10
      },
      "id": 2,
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      },
      "targets": [
        {
          "datasource": {
            "type": "grafana-bigquery-datasource",
            "uid": "${DS_BIGQUERY}"
          },
          "format": "time_series",
          "rawSql": "SELECT\\n  TIMESTAMP_TRUNC(observed_at, HOUR) AS time,\\n  SAFE_DIVIDE(COUNTIF(LOWER(overall_status) IN ('pass','ok')), COUNT(*)) * 100 AS value\\nFROM `${bq_project}.${bq_dataset}.release_health_events`\\nWHERE observed_at >= TIMESTAMP_SUB(CURRENT_TIMESTAMP(), INTERVAL 24 HOUR)\\nGROUP BY time\\nORDER BY time",
          "refId": "A"
        }
      ],
      "title": "BigQuery: Synthetic Overall Pass Rate (24h)",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "grafana-bigquery-datasource",
        "uid": "${DS_BIGQUERY}"
      },
      "description": "Counts required route SLO breaches from `route_check_events` (hourly buckets).",
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 10
      },
      "id": 3,
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      },
      "targets": [
        {
          "datasource": {
            "type": "grafana-bigquery-datasource",
            "uid": "${DS_BIGQUERY}"
          },
          "format": "time_series",
          "rawSql": "SELECT\\n  TIMESTAMP_TRUNC(observed_at, HOUR) AS time,\\n  COUNTIF(breached AND required) AS value\\nFROM `${bq_project}.${bq_dataset}.route_check_events`\\nWHERE observed_at >= TIMESTAMP_SUB(CURRENT_TIMESTAMP(), INTERVAL 24 HOUR)\\nGROUP BY time\\nORDER BY time",
          "refId": "A"
        }
      ],
      "title": "BigQuery: Required Route SLO Breaches (24h)",
      "type": "timeseries"
    },
    {
      "datasource": {
        "type": "grafana-bigquery-datasource",
        "uid": "${DS_BIGQUERY}"
      },
      "description": "Top route SLO breach keys in the last 7 days.",
      "fieldConfig": {
        "defaults": {},
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 18
      },
      "id": 4,
      "options": {
        "showHeader": true,
        "sortBy": [
          {
            "desc": true,
            "displayName": "breaches"
          }
        ]
      },
      "targets": [
        {
          "datasource": {
            "type": "grafana-bigquery-datasource",
            "uid": "${DS_BIGQUERY}"
          },
          "format": "table",
          "rawSql": "SELECT\\n  breach_key,\\n  endpoint_key,\\n  breach_type,\\n  COUNTIF(breached) AS breaches,\\n  COUNT(*) AS total\\nFROM `${bq_project}.${bq_dataset}.route_check_events`\\nWHERE observed_at >= TIMESTAMP_SUB(CURRENT_TIMESTAMP(), INTERVAL 7 DAY)\\n  AND required\\nGROUP BY breach_key, endpoint_key, breach_type\\nORDER BY breaches DESC\\nLIMIT 20",
          "refId": "A"
        }
      ],
      "title": "BigQuery: Top Route Breaches (7d)",
      "type": "table"
    },
    {
      "datasource": {
        "type": "grafana-bigquery-datasource",
        "uid": "${DS_BIGQUERY}"
      },
      "description": "Trend metrics exported into `quality_trend_events`. Metric values are stored as STRING; cast with SAFE_CAST for numeric charts.",
      "fieldConfig": {
        "defaults": {
          "unit": "percent"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 18
      },
      "id": 5,
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom"
        }
      },
      "targets": [
        {
          "datasource": {
            "type": "grafana-bigquery-datasource",
            "uid": "${DS_BIGQUERY}"
          },
          "format": "time_series",
          "rawSql": "SELECT\\n  TIMESTAMP_TRUNC(observed_at, DAY) AS time,\\n  metric AS metric,\\n  SAFE_CAST(metric_value AS FLOAT64) AS value\\nFROM `${bq_project}.${bq_dataset}.quality_trend_events`\\nWHERE observed_at >= TIMESTAMP_SUB(CURRENT_TIMESTAMP(), INTERVAL 30 DAY)\\n  AND classification = 'quality_trend'\\n  AND period = '$trend_period'\\n  AND metric IN ('strict_pass_rate','synthetic_overall_pass_rate','quarantine_pass_rate')\\nORDER BY time",
          "refId": "A"
        }
      ],
      "title": "BigQuery: Quality Trend Metrics (30d)",
      "type": "timeseries"
    }
  ],
  "refresh": "30s",
  "schemaVersion": 39,
  "style": "dark",
  "tags": [
    "vibecity",
    "ci",
    "synthetic",
    "observability"
  ],
